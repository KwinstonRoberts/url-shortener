<% include partials/_header %>
  <% if(!user){%>
    <p>You must be logged in to see your shortened urls </p>
    <%}else if(!urls){%>
    <p>You don't have any urls. why not make one below!</p>
    <%}else{%>
    <% for(x in urls){ %>
      <p><%=x%> ~> <a href='<%=urls[x]%>'><%= urls[x] %></a> <a href='urls/<%=x%>'> Edit </a> </p>
      <form method="POST" action=<%="/urls/" + x + "/delete"%>>
        <button type='submit'>Delete</button>
      </form>
      <%}%>
      <a href='urls/new'> shorten new url </a>
      <%}%>
  </body>
</html>